<view class="info-page">
  <!-- 页面顶部标题 -->
  <view class="info-header">
    <text>{{title}}</text>
  </view>
  
  <!-- 信息列表 -->
  <view class="info-list">
    <van-cell wx:for="{{infoList}}" wx:key="key" title="{{item.key}}" value="{{item.value}}"></van-cell>
  </view>
  
  <!-- 底部导航栏 -->
  <van-tabbar active="{{activeTabbar}}" bind:change="onChangeTabbar" active-color="#07c160">
    <van-tabbar-item icon="home-o" text="首页"></van-tabbar-item>
    <van-tabbar-item icon="user-o" text="信息"></van-tabbar-item>
  </van-tabbar>
</view>
